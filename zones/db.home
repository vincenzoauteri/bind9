;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ns.home. admin.home. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
	IN	NS	ns.home.
ns.home.	IN	A       192.168.1.7	
pi1.home.	IN	A       192.168.1.7	
pi2.home.	IN	A       192.168.1.131	
vbox.home.	IN	A       192.168.1.130	
